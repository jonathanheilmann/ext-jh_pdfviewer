<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sPARAMS>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:viewerParams</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.pdfFile>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:pdfFile</label>
							<config>
								<type>inline</type>
								<appearance type="array">
									<createNewRelationLinkTitle>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:createNewRelationLinkTitle</createNewRelationLinkTitle>
									<enabledControls type="array">
										<delete>1</delete>
										<dragdrop>1</dragdrop>
										<hide>1</hide>
										<info>1</info>
										<localize>1</localize>
										<sort>0</sort>
									</enabledControls>
									<headerThumbnail type="array">
										<field>uid_local</field>
										<height>45c</height>
										<width>45</width>
									</headerThumbnail>
									<useSortable>0</useSortable>
								</appearance>
								<foreign_types>
									<numIndex index="5">
										<showitem>--palette--;LLL:EXT:lang/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,--palette--;;filePalette</showitem>
									</numIndex>
								</foreign_types>
								<behaviour type="array">
									<localizationMode>select</localizationMode>
									<localizeChildrenAtParentLocalization>1</localizeChildrenAtParentLocalization>
								</behaviour>
								<filter type="array">

								</filter>
								<foreign_field>uid_foreign</foreign_field>
								<foreign_label>uid_local</foreign_label>
								<foreign_match_fields type="array">
									<fieldname>pdfFile</fieldname>
								</foreign_match_fields>
								<foreign_selector>uid_local</foreign_selector>
								<foreign_selector_fieldTcaOverride type="array">
									<config type="array">
										<appearance type="array">
											<elementBrowserAllowed>pdf</elementBrowserAllowed>
											<elementBrowserType>file</elementBrowserType>
										</appearance>
									</config>
								</foreign_selector_fieldTcaOverride>
								<foreign_sortby>sorting_foreign</foreign_sortby>
								<foreign_table>sys_file_reference</foreign_table>
								<foreign_table_field>tablenames</foreign_table_field>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.pdfFile>

					<settings.format>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:format</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">gif</numIndex>
										<numIndex index="1">gif</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">jpg</numIndex>
										<numIndex index="1">jpg</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">png</numIndex>
										<numIndex index="1">png</numIndex>
									</numIndex>
								</items>
								<default>jpg</default>
							</config>
						</TCEforms>
					</settings.format>

					<settings.width>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:width</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>6</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.width>
					<settings.height>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:height</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>6</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.height>

					<settings.startpage>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:startpage</label>
							<config>
								<type>input</type>
								<size>3</size>
								<max>3</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.startpage>
					<settings.endpage>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:endpage</label>
							<config>
								<type>input</type>
								<size>3</size>
								<max>3</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.endpage>

					<settings.hideTitle>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:hideTitle</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.hideTitle>
					<settings.hideIndex>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:hideIndex</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.hideIndex>
					<settings.hideNavigation>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:hideNavigation</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.hideNavigation>
					<settings.imgNavigationPos>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.label</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.ts</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.disable</numIndex>
										<numIndex index="1">disabled</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.top</numIndex>
										<numIndex index="1">top</numIndex>
									</numIndex>
									<!--<numIndex index="3">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.right</numIndex>
										<numIndex index="1">right</numIndex>
									</numIndex>-->
									<numIndex index="4">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.bottom</numIndex>
										<numIndex index="1">bottom</numIndex>
									</numIndex>
									<!--<numIndex index="5">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imgNavigationPos.left</numIndex>
										<numIndex index="1">left</numIndex>
									</numIndex>-->
								</items>
								<default>ts</default>
							</config>
						</TCEforms>
					</settings.imgNavigationPos>
					<!--<settings.hideImageNavigation>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:hideImageNavigation</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.hideImageNavigation>-->
					<settings.hideDownloadAdvice>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:hideDownloadAdvice</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.hideDownloadAdvice>

					<settings.addparams>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:addparams</label>
							<config>
								<type>input</type>
								<size>50</size>
								<max>50</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.addparams>

					<settings.imparams>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:imparams</label>
							<config>
								<type>input</type>
								<size>50</size>
								<max>80</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.imparams>

					<settings.linkTo>
						<TCEforms>
							<label>LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:linkTo.label</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:linkTo.none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:linkTo.link2doc</numIndex>
										<numIndex index="1">link2doc</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:jh_pdfviewer/Resources/Private/Language/locallang.xml:linkTo.magnificpopup</numIndex>
										<numIndex index="1">mfp</numIndex>
									</numIndex>
								</items>
								<default>none</default>
							</config>
						</TCEforms>
					</settings.linkTo>
				</el>
			</ROOT>
		</sPARAMS>


	</sheets>
</T3DataStructure>
